{
  "name": "joola.io",
  "preferGlobal": false,
  "version": "3.0.0",
  "author": "Joola <info@joo.la>",
  "description": "Joola Analytics Engine",
  "engine": "node >= 0.10.x",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/joola/joola.io.git"
  },
  "contributors": [
    {
      "name": "Itay Weinberger",
      "email": "itay@joo.la",
      "url": "http://joo.la"
    },
    {
      "name": "Or Weinberger",
      "email": "or@joo.la",
      "url": "http://joo.la"
    }
  ],
  "scripts": {
    "test": "mocha --reporter spec",
    "citest": "mocha > test-reports.xml",
    "start": "node joola.js",
    "debug": "npm-inspector && npm start && open \"http://http://127.0.0.1:8080/debug?port=5858\"",
    "monitor": "mocha --reporter min --watch",
    "debugtest": "mocha --debug-brk --reporter spec",
    "coverage": "mochacoverage",
		"blanket": {
			"pattern": "test",
			"data-cover-never": "node_modules"
		},
		"coveralls":"mocha --require blanket --reporter mocha-lcov-reporter | node ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "data",
    "analytics",
    "big-data",
    "visualization",
    "noSQL"
  ],
  "dependencies": {

  },
  "devDependencies": {
    "coveralls": "2.x",
    "mocha-lcov-reporter": "0.x",
    "should": "1.x",
    "mocha": "1.x",
    "nodecoverage": "0.x",
    "sinon": "1.x",
    "chai": "1.x",
    "sinon-chai": "2.x",
    "mochawrapper": "0.x",
		"blanket":"1.x"
  }
}
